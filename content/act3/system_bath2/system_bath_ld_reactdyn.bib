@article{mancho_2006,
	title = {A tutorial on dynamical systems concepts applied to {Lagrangian} transport in oceanic flows defined as finite time data sets: {Theoretical} and computational issues},
	volume = {437},
	issn = {03701573},
	shorttitle = {A tutorial on dynamical systems concepts applied to {Lagrangian} transport in oceanic flows defined as finite time data sets},
	url = {http://linkinghub.elsevier.com/retrieve/pii/S0370157306003401},
	doi = {10.1016/j.physrep.2006.09.005},
	abstract = {In the past 15 years the framework and ideas from dynamical systems theory have been applied to a variety of transport and mixing problems in oceanic ﬂows. The motivation for this approach comes directly from advances in observational capabilities in oceanography (e.g., drifter deployments, remote sensing capabilities, satellite imagery, etc.) which reveal space–time structures that are highly suggestive of the structures one visualizes in the global, geometrical study of dynamical systems theory.},
